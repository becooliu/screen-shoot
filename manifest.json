{
  "manifest_version": 3,
  "name": "capture webpage",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "downloads", "storage"],
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_popup": "./src/popup/popup.html",
    "default_icon": "icons/128.png"
  },

  "icons": {
    "128": "icons/128.png"
  }
}
